<!--The model expression at the top of the file specifies that the view 
    will receive a sequence of Product objects from the action method as its model data.-->
@model IEnumerable<Yacht>
@foreach (var p in Model)
{
    <div>
        <h3>@p.Name</h3>
        @p.Description
        <h4>@p.Price.ToString("c")</h4>
    </div>
}
